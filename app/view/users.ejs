<% include partials/head.ejs %>
    <title>Log in - CycLove</title>
</head>
<body>
    <header>
        <nav>
            <ul>
                <li><a href="/"><img src="/static/media/images/cyclove-logo.svg" alt="Logo CycLove"></a></li>
                <li><a href="/">Log in</a></li>
            </ul>
        </nav>
    </header>
    <main>
        <section>
          <% include partials/filter.ejs %>
          <% for (let i = 0; i < users.length; i++) { %>
            <a href="<%= users[i].id %>">
              <p><%= users[i].firstName %></p>
            </a>
          <% } %>
        </section>
    </main>
    <% include partials/footer.ejs %>
</body>
</html>
