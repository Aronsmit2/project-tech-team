<% include partials/head.ejs %>
    <title>Bar Locations - CycLove</title>
</head>
<body>
    <header>
        <nav>
            <ul>
                <li><a href="/"><img src="/static/media/images/cyclove-logo.svg" alt="Logo CycLove"></a></li>
                <li><a href="/log-out">Logout</a></li>
            </ul>
        </nav>
    </header>
    <h1> Bar Locations</h1>
    <main>
        <section class="card-grid">
            <% if(barLocations.includes("a") == "true"){ %>
                <% for (let i = 0; i < barLocations.length; i++) { %> 
                    <article class="card">
                        <%= barLocations.status[i] %>
                    </article>
            <% }} else{ %>  
                <p>nope</p>
            <% } %>
        </section>
    </main>
    <% include partials/footer.ejs %>
</body>
</html>